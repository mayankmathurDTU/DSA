https://www.geeksforgeeks.org/problems/height-of-binary-tree/1

/*
class Node {
public:
    int data;
    Node* left;
    Node* right;

    Node(int val) {
        data = val;
        left = right = NULL;
    }
};
*/
class Solution {
  public:
    int solve(Node* node){
        if(node == NULL)return 0;
        return 1 + max(solve(node->left),solve(node->right));
    }
    int height(Node* node) {
        return solve(node)-1;
    }
};
