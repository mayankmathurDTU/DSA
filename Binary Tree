PreOrderTraversal of Binary Tree
GFG Question Link : https://www.geeksforgeeks.org/problems/preorder-traversal/1

/*
struct Node
{
    int data;
    struct Node* left;
    struct Node* right;

    Node(int x){
        data = x;
        left = right = NULL;
    }
};
*/
class Solution {
  public:
    void preOrderTraversal(Node* root,vector<int>& ans){
        if(root==NULL)return;
        ans.push_back(root->data);
        preOrderTraversal(root->left,ans);
        preOrderTraversal(root->right,ans);
    }
    vector<int> preorder(Node* root) {
        vector<int> ans;
        //preOrderTraversal(root,ans);
        if(root==NULL)return ans;
        stack<Node*> s;
        s.push(root);
        while(!s.empty()){
            auto top = s.top();
            s.pop();
            ans.push_back(top->data);
            if(top->right)s.push(top->right);
            if(top->left)s.push(top->left);
        }
        return ans;
    }
};
